<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vision Logistics</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/gsap.min.js"></script>
</head>
<body>
    <%- include('./partials/top-menu') %>

    <div class="start-container">
        <img src="/Assets/img/newspaper.jpg"/>
        <div class="overlaypicture"></div>
        <div class="starttext-container">
            <p class="start-text">NEWS & UPDATES</p>
        </div>
    </div>
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 60" preserveAspectRatio="none" class="section-divider">
        <path fill="white" d="
            M0 0
            H100
            V80
            H0
            Z
            M0 1
            Q20 40 50 40
            Q80 40 100 1
            V0 H0 Z"
        />
    </svg>
    <!-- news.ejs -->
    <div class="api-container">
        <% newsItems.forEach(newsItem => { %>
            <div class="image-block">
                <img src="<%= newsItem.imageUrl %>" alt="<%= newsItem.title %>">
                <div class="text-block">
                <h3><%= newsItem.title %></h3>
                <p><%= newsItem.detail %></p>
                <!-- Add other fields as needed -->
            </div>
        </div>
    <% }); %>
</div>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 60" preserveAspectRatio="none" class="section-divider3">
    <path fill="#0B1C48" d="
        M0 0
        H100
        V80
        H0
        Z
        M0 1
        Q20 40 50 40
        Q80 40 100 1
        V0 H0 Z"
    />
</svg>
